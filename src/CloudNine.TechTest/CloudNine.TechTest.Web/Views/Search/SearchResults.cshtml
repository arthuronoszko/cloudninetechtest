@model CloudNine.TechTest.Web.ViewModels.SearchResultsViewModel

<div>
    Resultat för <strong> @Model.GenreSearchString</strong>:

    <ul>
        @foreach (var track in Model.Tracks) {
            {
                <li>
                    <p> <a href="@track.ListeningLink"> @track.Name </a> </p>
                    <p> @track.Artists </p>
                    <p> @track.DanceabilityScore </p>
                </li>
            }
        }
    </ul>
</div>